<AnchorPane fx:id="anp" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labsocialnetworkv2.controller.MainPageController">
    <TableView fx:id="tableView" layoutX="14.0" layoutY="100.0" prefHeight="200.0" prefWidth="373.0">
      <columns>
          <TableColumn fx:id="tableColumnUserId" prefWidth="75.0" text="User Id" />
          <TableColumn fx:id="tableColumnFirstName" prefWidth="102.0" text="First Name" />
          <TableColumn fx:id="tableColumnLastName" prefWidth="91.0" text="Last Name" />
          <TableColumn fx:id="tableColumnBirthday" prefWidth="104.0" text="Birthday" />
      </columns>
    </TableView>
    <Button layoutX="162.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleRemoveButton" text="Remove" />
    <Button layoutX="430.0" layoutY="188.0" mnemonicParsing="false" text="Add Friend" />
    <Label fx:id="welcomeText" />
    <Button fx:id="loginButton" layoutX="469.0" layoutY="258.0" mnemonicParsing="false" text="Login"  onAction="#onLoginButtonClick" />
   <TextField fx:id="loginTextField" layoutX="418.0" layoutY="219.0" />
   <Label layoutX="407.0" layoutY="188.0" text="User Id" textFill="#5457b0">
      <font>
         <Font name="Comic Sans MS Bold Italic" size="16.0" />
      </font>
   </Label>
</AnchorPane>
