         <Font size="17.0" />
      </font></Button>
    <Button fx:id="buttonSendRequest" layoutX="42.0" layoutY="443.0" mnemonicParsing="false" onAction="#handleSendRequestButton" prefHeight="43.0" prefWidth="217.0" style="-fx-background-color: #40445a; -fx-background-radius: 15;" text="Send Friend Request" textFill="#e8fff5">
