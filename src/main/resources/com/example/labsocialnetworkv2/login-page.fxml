<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.labsocialnetworkv2.controller.LoginPageController">
    <TableView fx:id="tableView" layoutX="29.0" layoutY="100.0" prefHeight="200.0" prefWidth="337.0">
      <columns>
          <TableColumn fx:id="tableColumnUserId" prefWidth="75.0" text="User Id"/>
          <TableColumn fx:id="tableColumnFirstName" prefWidth="89.0" text="First Name"/>
          <TableColumn fx:id="tableColumnLastName" prefWidth="90.0" text="Last Name"/>
          <TableColumn fx:id="tableColumnBirthday" prefWidth="82.0" text="Birthday"/>
      </columns>
    </TableView>
    <Label layoutX="55.0" layoutY="34.0" prefHeight="55.0" prefWidth="290.0" text="Social Network" textFill="#7070fc">
        <font>
          <Font name="Chilanka Regular" size="36.0"/>
        </font>
    </Label>
    <Button fx:id="buttonLogin" layoutX="452.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Login"/>
    <TextField fx:id="textFieldUserId" layoutX="394.0" layoutY="163.0"/>
    <Label layoutX="394.0" layoutY="136.0" text="User Id"/>
</AnchorPane>
