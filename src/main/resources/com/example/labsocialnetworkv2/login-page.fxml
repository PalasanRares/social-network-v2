<AnchorPane prefHeight="400.0" prefWidth="519.0" style="-fx-background-color: DIMGRAY;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labsocialnetworkv2.controller.LoginPageController">
