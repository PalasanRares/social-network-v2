        <font>
          <Font size="50.0" />
        </font>
      <textFill>
         <LinearGradient cycleMethod="REFLECT" endX="1.0" endY="1.0" startY="0.12121212166367146">
            <stops>
               <Stop>
                  <color>
                     <Color red="0.28272905945777893" green="0.5639194250106812" blue="0.7789473533630371" />
                  </color>
               </Stop>
               <Stop offset="1.0">
                  <color>
                     <Color red="1.0" green="1.0" blue="1.0" />
                  </color>
               </Stop>
            </stops>
         </LinearGradient>
      </textFill>
    </Label>
    <Button fx:id="buttonLogin" layoutX="271.0" layoutY="251.0" mnemonicParsing="false" onAction="#handleLoginButton" text="Login" textFill="#273d8f">
      <font>
         <Font name="Comic Sans MS Bold Italic" size="15.0" />
      </font></Button>
    <TextField fx:id="textFieldUsername" layoutX="224.0" layoutY="148.0" />
    <Label layoutX="135.0" layoutY="148.0" text="Username" textFill="WHITE">
      <font>
         <Font name="Comic Sans MS Bold Italic" size="15.0" />
      </font></Label>
   <Label layoutX="135.0" layoutY="195.0" text="Password" textFill="WHITE">
      <font>
         <Font name="Comic Sans MS Bold Italic" size="15.0" />
      </font>
   </Label>
   <PasswordField fx:id="textFieldPassword" layoutX="225.0" layoutY="193.0" />
   <Hyperlink fx:id="signUp" layoutX="274.0" layoutY="297.0" onAction="#goSignUp" text="Sign Up" textFill="WHITE" />
</AnchorPane>
